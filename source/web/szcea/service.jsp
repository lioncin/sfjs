<script type="text/javascript">
var LIMIT = 10;

    $(function(){
        let type = parseInt(location.href.split('service/')[1]);
        showType(type);

        $('.service-item').click(function(){
            let t = $(this).attr('type');
            if(t=='29' || t=='30'){
                getDongtai(LIMIT,t);
            }else{
                showType(t);
            }
            
        });
        
    });

    function showType(type){
        $('.service-item').removeClass('current');
        $('.service-content').addClass('hidden');
        $('.service-item').each(function(){
            let t = $(this).attr('type');
            if(t == type){
                $(this).addClass('current');
            }
        });
        $('.service-content').each(function(){
            let t = $(this).attr('type');
            if(t == type){
                $(this).removeClass('hidden');
            }
        });
    }

    function getDongtai(limit,type){
        var url = '<c:url value="/szcea/getYuGao" />';
        $.ajax({
            'url':url,
            'type':'POST',
            'data':{
                limit:limit,
                type:type
            },
            success:function(data,status){
                var d = data['data']['data'];
                if(typeof d != 'undefined'){
                    showDongtai(d,type);
                }
                
            }
        });
    }

    function showDongtai(data,type){
        var _html = '';
        var size = data.length;
        for(let i=0; i<size; i++){
             let url = '#'; //content
            var d = data[i];
            if(type=='30'){
               url = 'http://'+ d['summary']; //school
            }

            _html += '<a class="pinpai-item" href="'+url+'">';
            if(typeof d['attachments'] != 'undefined'  && typeof d['attachments'][0] && typeof d['attachments'][0]['path'] !='undefined'){
                var path = d['attachments'][0]['path'];
                _html += '  <div class="img"><img src="<f:download path="'+path+'" />" /></div>';
            }else{
                _html += '  <div class="img"><img src="<c:url value="/szcea/statics/images/school.jpg"/>" /></div>';
            }
            _html += '  <div class="stext">'+d['name']+'</div>';
            _html += '</a>';
        }
        _html += '<div class="cls"></div>';

        $('.service-item').removeClass('current');
        $('.service-content').addClass('hidden');
        $('.service-item').each(function(){
            let t = $(this).attr('type');
            if(t == type){
                $(this).addClass('current');
            }
        });
        $('.service-content').each(function(){
            let t = $(this).attr('type');
            if(t == type){
                $(this).html(_html);
                $(this).removeClass('hidden');
            }
        });
    }

</script>